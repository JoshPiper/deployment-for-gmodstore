{
  "name": "@doctor_internet/deployment-for-gmodstore",
  "version": "1.0.0",
  "description": "GitHub Action for deploying to GModStore.",
  "main": "index.js",
  "scripts": {
    "clean-dist": "((ls dist > /dev/null 2>&1) && rm -r ./dist) || true",
    "clean-modules": "((ls node_modules > /dev/null 2>&1) && rm -r ./node_modules) || true",
    "clean": "npm run clean-dist && npm run clean-modules",
    "compile": "ncc build index.js --license licenses.txt",
    "build": "npm run clean && npm i && npm run compile"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com//JoshPiper/GModStore-Deployment.git"
  },
  "keywords": [],
  "author": "John Internet <internet@doctor-internet.dev>",
  "license": "ISC",
  "dependencies": {
    "@actions/core": "^1.10.0",
    "form-data-encoder": "^1.9.0",
    "formdata-node": "^4.4.1",
    "node-fetch": "^2.7.0",
    "semver": "^7.5.4",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@tsconfig/node20": "^20.1.2",
    "@types/chai": "^4.3.6",
    "@types/mocha": "^10.0.2",
    "@types/node": "^20.8.2",
    "@types/node-fetch": "^2.6.6",
    "@types/semver": "^7.5.3",
    "@types/sinon": "^10.0.18",
    "@types/uuid": "^9.0.4",
    "@vercel/ncc": "^0.34.0",
    "chai": "^4.3.10",
    "mocha": "^10.2.0",
    "nyc": "^15.1.0",
    "semantic-release": "^22.0.5",
    "sinon": "^16.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "yaml": "^2.3.2"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
